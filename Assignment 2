interface addfloat
{
    public void add();
}
public class addition_float_usinginterface implements addfloat
{
    public void add()
    {
        float c=20.0f;
        float a=100;
        System.out.println(a+b);
    }
    public static void main(String args[])
    {
        addition_float_usinginterface obj= new addition_float_usinginterface();
       obj.add();
    }
    
}
